{{#each users}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{firstName}} {{lastName}}
    </h2>
    <span>E-Mail: {{email}}</span>
    <a href="/admin/delete/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-trash"></i>
      </span>
    </a>
  </div>
{{/each}}
